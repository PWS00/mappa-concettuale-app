{
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/generate",
      "dest": "api/main.py"
    },
    {
      "src": "/script.js",
      "dest": "static/script.js"
    },
    {
      "src": "/style.css",
      "dest": "static/style.css"
    },
    {
      "src": "/(.*)",
      "dest": "static/index.html"
    }
  ]
}
